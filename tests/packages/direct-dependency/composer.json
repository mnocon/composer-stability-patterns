{
    "name": "mareknocon/direct-dependency",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mareknocon\\VendorStabilityTest\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Marek Noco≈Ñ",
            "email": "mnocon@users.noreply.github.com"
        }
    ],
    "require": {
        "mareknocon/indirect-dependency": "~1.0@dev"
    },
    "require-dev": {
        "mareknocon/composer-vendor-stability": "~1.0@dev"
    },
    "repositories": {
        "plugin": {
            "type": "path",
            "url": "../../.."
        },
        "indirect-dependency": {
            "type": "path",
            "url": "../indirect-dependency"
        },
        "second-level-indirect-dependency": {
            "type": "path",
            "url": "../second-level-indirect-dependency"
        }
    },
    "config": {
        "allow-plugins": {
            "mareknocon/composer-vendor-stability": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        },
        "minimum-stability": {
            "mareknocon/*": "dev"
        }
    },
    "minimum-stability": "stable"
}
